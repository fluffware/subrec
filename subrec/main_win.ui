<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <object class="GtkAction" id="about_action">
    <property name="label" translatable="yes">_About</property>
    <property name="short_label" translatable="yes">About</property>
    <property name="stock_id">gtk-about</property>
    <signal name="activate" handler="about_action_activate_cb" swapped="no"/>
  </object>
  <object class="GtkIconFactory" id="app_iconfactory">
    <sources>
      <source stock-id="subrec-red-lamp" filename="red_lamp_normal.png"/>
      <source stock-id="subrec-red-lamp" state="prelight" filename="red_lamp_active.png"/>
      <source stock-id="subrec-green-lamp" state="normal" filename="green_lamp_normal.png"/>
      <source stock-id="subrec-green-lamp" state="prelight" filename="green_lamp_active.png"/>
      <source stock-id="subrec-yellow-lamp" state="normal" filename="yellow_lamp_normal.png"/>
      <source stock-id="subrec-yellow-lamp" state="prelight" filename="yellow_lamp_active.png"/>
    </sources>
  </object>
  <object class="GtkAction" id="collapse_all_action">
    <property name="label" translatable="yes">_Collapse All</property>
    <property name="short_label" translatable="yes">Collapse</property>
    <property name="stock_id">gtk-remove</property>
    <signal name="activate" handler="collapse_all_action_activate_cb" swapped="no"/>
  </object>
  <object class="GtkAction" id="expand_all_action">
    <property name="label" translatable="yes">_Expand All</property>
    <property name="short_label" translatable="yes">Expand</property>
    <property name="stock_id">gtk-add</property>
    <signal name="activate" handler="expand_all_action_activate_cb" swapped="no"/>
  </object>
  <object class="GtkAction" id="open_assetmap_action">
    <property name="label" translatable="yes">_Open ASSETMAP</property>
    <property name="stock_id">gtk-open</property>
    <property name="icon_name">Open ASSETMAP</property>
    <signal name="activate" handler="open_assetmap_action_cb" swapped="no"/>
  </object>
  <object class="GtkAction" id="play_clip">
    <property name="label" translatable="yes">Play</property>
    <property name="short_label" translatable="yes">Play</property>
    <property name="stock_id">gtk-media-play</property>
    <signal name="activate" handler="play_action_activate_cb" swapped="no"/>
  </object>
  <object class="GtkAction" id="preferences_action">
    <property name="label" translatable="yes">_Preferences</property>
    <property name="short_label" translatable="yes">Prefs</property>
    <property name="stock_id">gtk-preferences</property>
    <signal name="activate" handler="preferences_action_activate_cb" swapped="no"/>
  </object>
  <object class="GtkAction" id="quit_action">
    <property name="label" translatable="yes">_Quit</property>
    <property name="stock_id">gtk-quit</property>
    <signal name="activate" handler="quit_action_cb" swapped="no"/>
  </object>
  <object class="GtkAction" id="record_clip">
    <property name="label" translatable="yes">Record</property>
    <property name="short_label" translatable="yes">Record</property>
    <property name="stock_id">gtk-media-record</property>
    <signal name="activate" handler="record_action_activate_cb" swapped="no"/>
  </object>
  <object class="GtkAction" id="save_action">
    <property name="label" translatable="yes">_Save</property>
    <property name="stock_id">gtk-save</property>
    <signal name="activate" handler="save_action_activate_cb" swapped="no"/>
  </object>
  <object class="GtkAction" id="set_working_directory_action">
    <property name="label" translatable="yes">Set _working directory</property>
    <property name="stock_id">gtk-save-as</property>
    <signal name="activate" handler="set_working_directory_action_activate_cb" swapped="no"/>
  </object>
  <object class="GtkAction" id="stop_clip">
    <property name="label" translatable="yes">Stop</property>
    <property name="short_label" translatable="yes">Stop</property>
    <property name="stock_id">gtk-media-stop</property>
    <signal name="activate" handler="stop_action_activate_cb" swapped="no"/>
  </object>
  <object class="GtkAdjustment" id="prefs_pre_silence_adjustment">
    <property name="upper">1</property>
    <property name="step_increment">0.01</property>
    <property name="page_increment">0.10000000000000001</property>
  </object>
  <object class="GtkAdjustment" id="prefs_silence_value_adjustment">
    <property name="lower">-90.010000000000005</property>
    <property name="step_increment">0.10000000000000001</property>
    <property name="page_increment">1</property>
  </object>
  <object class="GtkTextBuffer" id="subtitle_text"/>
