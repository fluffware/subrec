<schemalist>
  <schema id="se.fluffware.apps.subrec" path="/recording/levels/" gettext-domain="subrec">
    

    <key name="silence-level" type="d">
      <default >0.001</default>
      <range min="0.00001" max="1.0"/>
      <summary>Silence level</summary>
      <description>
	Power below this level i considered silence
      </description>
    </key>
    <key name="warning-level" type="d">
      <default >0.001</default>
      <range min="0.00001" max="1.0"/>
      <summary>Warning level</summary>
      <description>
	The yellow warning lamp is lit at this level
      </description>
    </key>
     <key name="pre-silence" type="t">
      <default >0</default>
      <range min="0" max="1000000000"/>
      <summary>Silence before</summary>
      <description>
	Length of silence before clip (ns)
      </description>
    </key>

    <key name="post-silence" type="t">
      <default >0</default>
      <range min="0" max="1000000000"/>
      <summary>Silence after</summary>
      <description>
	Length of silence after clip (ns)
      </description>
    </key>
    
  </schema>
</schemalist>
